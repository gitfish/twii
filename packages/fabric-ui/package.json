{
  "name": "@twii/fabric-ui",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@twii/common": "^1.0.1",
    "@twii/common-ui": "^1.0.1",
    "@twii/router": "^1.0.1",
    "@uifabric/styling": "^5.22.0",
    "@uifabric/utilities": "^5.23.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.21.0",
    "office-ui-fabric-react": "^5.79.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/react": "^16.3.4",
    "del": "^3.0.0",
    "jest": "^22.1.4",
    "jsdom": "^11.5.1",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^21.2.3",
    "typescript": "^2.8.0"
  },
  "scripts": {
    "clean": "node task/clean",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts?(x)"
    ],
    "coverageDirectory": "__coverage__",
    "transform": {
      ".(ts|tsx)": "../../node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*Test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
