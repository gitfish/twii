{
  "name": "@twii/common",
  "description": "twii Common lib",
  "version": "1.0.4",
  "license": "MIT",
  "dependencies": {
    "@twii/router": "^1.0.4",
    "lodash.isfunction": "^3.0.8",
    "mobx": "^4.1.1",
    "moment": "^2.21.0",
    "qs": "^6.5.1",
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/react": "^16.3.4",
    "del": "^3.0.0",
    "jest": "^22.1.4",
    "jsdom": "^11.5.1",
    "ts-jest": "^21.2.3",
    "typescript": "^2.8.0"
  },
  "scripts": {
    "clean": "node task/clean",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts?(x)"
    ],
    "coverageDirectory": "__coverage__",
    "transform": {
      ".(ts|tsx)": "../../node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*Test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
