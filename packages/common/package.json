{
  "name": "@twii/common",
  "description": "twii Common lib",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@uifabric/styling": "^5.19.0",
    "@uifabric/utilities": "^5.20.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
    "qs": "^6.5.1",
    "roota": "^1.7.0",
    "office-ui-fabric-react": "^5.69.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "moment": "^2.21.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "del": "^3.0.0",
    "jest": "^22.1.4",
    "jsdom": "^11.5.1",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^21.2.3",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "clean": "node task/clean",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts?(x)"
    ],
    "coverageDirectory": "__coverage__",
    "transform": {
      ".(ts|tsx)": "../../node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*Test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
