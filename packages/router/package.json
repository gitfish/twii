{
  "name": "@twii/router",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "node task/clean",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "lodash.isfunction": "^3.0.8",
    "lodash.isstring": "^4.0.1",
    "path-to-regexp": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.3.4",
    "del": "^3.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts?(x)"
    ],
    "coverageDirectory": "__coverage__",
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*Test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
