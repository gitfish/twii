{
  "name": "@navish/ui-core",
  "version": "1.0.0",
  "description": "UI Core Components",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "node task/clean",
    "build": "tsc",
    "build-watch": "tsc-watch",
    "test": "jest"
  },
  "dependencies": {
    "@uifabric/styling": "^5.10.1",
    "@uifabric/utilities": "^5.4.0",
    "@navish/core": "^1.0.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "mobx-react": "^4.3.5",
    "office-ui-fabric-react": "^5.40.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "roota": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "del": "^3.0.0",
    "react-test-renderer": "^16.2.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts?(x)"
    ],
    "coverageDirectory": "__coverage__",
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*Test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
