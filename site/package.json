{
  "name": "@twii/site",
  "version": "3.0.0",
  "description": "Twii Site",
  "main": "app/index.js",
  "scripts": {
    "clean": "node task/clean.js",
    "start": "node app",
    "build": "webpack",
    "build-watch": "webpack --progress --watch",
    "build-watch-mock": "webpack --progress --watch --env.configName=mock",
    "test": "jest"
  },
  "author": "analystdesktop@border.gov.au",
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts?(x)"
    ],
    "coverageDirectory": "__coverage__",
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*Test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "package.json$": "<rootDir>/package.json"
    },
    "globals": {
      "AppConfig": {
        "basePath": "/test"
      }
    }
  },
  "dependencies": {
    "@microsoft/loader-load-themed-styles": "^1.6.1",
    "@types/file-saver": "^0.0.1",
    "@types/jest": "^21.1.5",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@uifabric/icons": "5.2.0",
    "@uifabric/merge-styles": "^5.10.0",
    "@uifabric/styling": "^5.15.1",
    "@uifabric/utilities": "^5.6.1",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.0",
    "css-loader": "^0.28.4",
    "cucumber": "^2.0.0-rc.9",
    "cucumber-html-reporter": "^0.4.0",
    "del": "^3.0.0",
    "docxtemplater": "^3.1.5",
    "express": "^4.15.4",
    "file-loader": "^0.11.2",
    "file-saver": "^1.3.3",
    "gulp": "^3.9.0",
    "gulp-cucumber": "^0.0.22",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.0",
    "jsdom": "^11.2.0",
    "jszip": "2.6.1",
    "jszip-utils": "^0.0.2",
    "less": "^2.7.2",
    "lodash.isequal": "^3.0.4",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.6.0",
    "office-ui-fabric-react": "^5.43.1",
    "path-to-regexp": "^1.7.0",
    "qs": "^6.5.1",
    "react": "^16.1.1",
    "react-arcgis": "^3.1.3",
    "react-dom": "^16.1.1",
    "react-image-gallery": "^0.8.3",
    "react-test-renderer": "^16.0.0",
    "read-text-file": "^1.1.0",
    "request": "^2.81.0",
    "roota": "^1.2.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.2",
    "typescript": "^2.6.1",
    "url-loader": "^0.5.9",
    "vis": "^4.20.1",
    "webpack": "^3.8.1",
    "xlsx": "^0.10.3",
    "xmldom": "^0.1.27",
    "yargs": "^9.0.1"
  }
}
